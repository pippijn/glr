terminals {
  0 : TOK_EOF	"<EOF>";
  1 : TOK_COMMA	",";
  2 : TOK_NAME;

  token(string) TOK_NAME;
}


nonterm(string) File	-> t:TemplateArgument;

(* Bug: 3 levels break the parser, 2 levels work fine. *)
nonterm NameStart	-> id:TOK_NAME;
nonterm NameMiddle	-> id:NameStart;
nonterm NameEnd		-> id:NameMiddle;

nonterm TemplateArgument {
  fun merge(l,r) { "Merge(" ^ l ^ "," ^ r ^ ")" }

  -> id:TOK_NAME	[Type];
  -> id:NameEnd		[NonType];
}
