terminals {
  0 : TOK_EOF	"<EOF>";
  1 : TOK_COMMA	",";
  2 : TOK_NAME;

  token(string) TOK_NAME;
}


nonterm(string) File	-> t:TemplateArgument;

nonterm Name	-> id:TOK_NAME;
nonterm NameRef	-> id:Name;

nonterm TemplateArgument {
  fun merge(l,r) { "Merge(" ^ l ^ "," ^ r ^ ")" }

  -> id:TOK_NAME	[Type];
  (* Bug: NameRef breaks the merge, just Name works fine. *)
  -> id:NameRef		[NonType];
}
