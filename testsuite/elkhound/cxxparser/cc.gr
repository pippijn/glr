terminals {
  0 : TOK_EOF	"<EOF>";
  1 : TOK_COMMA	",";
  2 : TOK_NAME;

  token(string) TOK_NAME;
}


nonterm(string) File		-> t:TemplateArgumentList;

nonterm Name			-> id:TOK_NAME;
nonterm TypeId			-> id:TOK_NAME;
nonterm PrimaryExpression	-> id:Name;
nonterm BinaryExpression	-> e:PrimaryExpression;

nonterm TemplateArgumentList {
  fun merge(l,r) { "Merge(" ^ l ^ "," ^ r ^ ")" }

  -> id:TypeId			[Type];
  -> id:BinaryExpression	[NonType];
}
